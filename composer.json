  "keywords": [
    "think",
    "thinkphp6",
    "apidoc",
    "api文档",
    "自动生成"
  ],
  "type": "think-extend",
  "authors": [
    {
      "name": "shuxian",
      "email": "winnie_xian@163.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ric/thinkphp6-helper":"^1.0"
  },
  "autoload": {
    "psr-4": {
      "ric\\apidoc\\": "src/"
    }
  },
  "extra": {
    "think": {
      "config": {
        "apidoc": "src/config/apidoc.php"
      }
    }
  }
}
